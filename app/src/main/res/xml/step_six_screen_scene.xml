<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/view4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="#110E23" />
        </Constraint>
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/imageView18"
            motion:layout_constraintEnd_toEndOf="@+id/view4"
            android:layout_width="wrap_content"
            android:id="@+id/imageView17"
            android:layout_marginTop="32dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView20" />
        <Constraint
            android:layout_height="384dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.657"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="558dp"
            android:id="@+id/imageView23"
            android:translationY="70dp"
            android:translationX="-20dp"
            motion:layout_editor_absoluteX="-121dp"
            motion:layout_editor_absoluteY="417dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/view4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@color/SkyBlue" />
        </Constraint>
        <Constraint
            android:id="@+id/imageView20"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="32dp"
            android:rotation="99"
            android:translationX="120dp"
            android:translationY="-150dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/imageView18"
            motion:layout_constraintEnd_toEndOf="@+id/view4"
            android:layout_width="wrap_content"
            android:id="@+id/imageView17"
            android:layout_marginTop="32dp"
            android:translationY="200dp" />
        <Constraint
            android:id="@+id/imageView18"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/view4"
            motion:layout_constraintStart_toStartOf="parent"
            android:translationY="150dp" />
        <Constraint
            android:id="@+id/imageView23"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="525dp"
            android:layout_height="366dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:translationY="300dp"
            motion:layout_constraintHorizontal_bias="0.603"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
    </ConstraintSet>
</MotionScene>